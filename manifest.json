{
  "manifest_version": 2,
 
  "name": "Grammar Police",

  "version": "0.1.0",
  "description": "A Chrome extension.",

  "browser_action": {
	"default_title": "Grammar Police",
  "default_popup": "popup.html"
	},
 
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://*/*", "http://*/*"]
  }
  ]

}